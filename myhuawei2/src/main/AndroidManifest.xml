<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myhuawei">

    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".base.StoreApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvp.MainActivity" />
        <activity android:name=".mvp.view.activity.HomeActivity" />
        <activity android:name=".mvp.view.activity.ApkManagementActivity" />
        <activity android:name=".mvp.view.activity.CleanCacheActivity" />
        <activity android:name=".mvp.view.activity.InstallAppInfoActivity" />
        <activity android:name=".mvp.view.activity.AppDetailActivity" />
        <activity android:name=".mvp.view.activity.AppMoreRecommendActivity" />
        <activity android:name=".mvp.view.activity.GalleryActivity" />
        <activity android:name=".mvp.view.activity.CategoryNewActivity"/>
        <activity android:name=".mvp.view.activity.CategorySubjectActivity"/>
        <activity android:name=".mvp.view.activity.CategorySubscribeActivity"/>
        <activity android:name=".mvp.view.activity.WebViewActivity"/>

    </application>

</manifest>